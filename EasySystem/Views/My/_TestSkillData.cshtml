@if (TempData["TestSkills"] != null)
{
        <div class="button-sec">
            @foreach (var i in (List<EasySystemAPI.Models.SkillType>)TempData["TestSkills"])
            {
                <a asp-controller="My" asp-action="CertificationTest" asp-route-id="@i.StId" class="button pulse-grow">@i.StName</a>
            }
        </div>
}
else
{
    <p>No Skill Found for this category</p>
}